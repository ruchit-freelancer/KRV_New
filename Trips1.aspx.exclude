<%@ Page Language="C#" MasterPageFile="~/KRV.master" AutoEventWireup="true" CodeFile="Trips1.aspx.cs"
    Inherits="Trips1" %>

<asp:Content ID="Content3" ContentPlaceHolderID="cphHeadTitle" runat="Server">
    Trips
</asp:Content>
<asp:Content ID="Content4" ContentPlaceHolderID="cphContent" runat="Server">
    <%--<telerik:RadCodeBlock ID="RadCodeBlock1" runat="server">

        <script type="text/javascript">
            //<![CDATA[

            function onTabSelecting(sender, args) {
                var tabStrip = $find("<%= rtsTrips.ClientID %>");
                var tabs = tabStrip.get_tabs();
                var tab = args.get_tab().get_text();
                if (tab == "Trips") {
                    tabs.getTab(1).set_enabled(false);
                    tabs.getTab(2).set_enabled(false);
                    tabs.getTab(3).set_enabled(false);
                    tabs.getTab(4).set_enabled(false);
                }
                if (tabStrip.get_selectedTab().get_text() == "Trips") {
                    tabs.getTab(1).set_enabled(false);
                    tabs.getTab(2).set_enabled(false);
                    tabs.getTab(3).set_enabled(false);
                    tabs.getTab(4).set_enabled(false);
                }
            }
            //]]>
        </script>

    </telerik:RadCodeBlock>--%>
    <telerik:RadAjaxPanel ID="RadAjaxPanel1" runat="server" LoadingPanelID="RadAjaxLoadingPanel1">
        <asp:HiddenField ID="hfTripID" runat="server" Value="0" />
        <telerik:RadTabStrip ID="rtsTrips" runat="server" Skin="Black" MultiPageID="RadMultiPage1"
            SelectedIndex="0" CssClass="tabStrip">
            <%-- OnClientTabSelecting="onTabSelecting"--%>
            <Tabs>
                <telerik:RadTab Text="Trips" PageViewID="rpvTrips">
                </telerik:RadTab>
                <telerik:RadTab Text="Trip Stops" PageViewID="rpvTripStops" Enabled="false">
                </telerik:RadTab>
                <telerik:RadTab Text="Trip Routes" PageViewID="rpvTripRoutes" Enabled="false">
                </telerik:RadTab>
                <telerik:RadTab Text="Trip Incidents" PageViewID="rpvTripIncidents" Enabled="false">
                </telerik:RadTab>
                <telerik:RadTab Text="State Travelled" PageViewID="rpvStateTravelled" Enabled="false">
                </telerik:RadTab>
            </Tabs>
        </telerik:RadTabStrip>
        <telerik:RadMultiPage ID="RadMultiPage1" runat="server" SelectedIndex="0">
            <telerik:RadPageView ID="rpvTrips" runat="server">
                <telerik:RadGrid ID="rgSearch" runat="server" AllowFilteringByColumn="True" AllowPaging="True"
                    AllowAutomaticDeletes="true" AllowAutomaticInserts="false" AllowAutomaticUpdates="true"
                    ShowStatusBar="true" AllowSorting="True" AutoGenerateColumns="False" CellSpacing="0"
                    DataSourceID="sdsAddEditRecords" GridLines="None" Skin="Sunset" OnInsertCommand="RadGrid1_InsertCommand"
                    OnUpdateCommand="RadGrid1_UpdateCommand" OnItemDeleted="RadGrid1_ItemDeleted">
                    <HeaderContextMenu CssClass="GridContextMenu GridContextMenu_Default">
                    </HeaderContextMenu>
                    <MasterTableView DataSourceID="sdsAddEditRecords" DataKeyNames="TripID" CommandItemDisplay="Top">
                        <EditFormSettings EditFormType="Template">
                            <FormTemplate>
                                <h1>
                                    New Trip</h1>
                                <hr />
                                <table cellpadding="2" cellspacing="2">
                                    <tr>
                                        <td align="left" valign="middle">
                                            Select Truck:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadComboBox ID="rcmbTrucks" runat="server" DataSourceID="sdsTrucks" DataTextField="TruckNo"
                                                DataValueField="TruckID" MarkFirstMatch="true" SelectedValue='<%# Eval("TruckID") %>'>
                                            </telerik:RadComboBox>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Select Driver:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadComboBox ID="rcmbDriver" runat="server" DataSourceID="sdsDrivers" DataTextField="DriverName"
                                                DataValueField="DriverID" MarkFirstMatch="true" SelectedValue='<%# Eval("DriverID") %>'>
                                            </telerik:RadComboBox>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Trip Date:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadDatePicker ID="rdtpTripDate" runat="server" TabIndex="3" Culture="English (United States)"
                                                Skin="Sunset" DbSelectedDate='<%# Bind("TripDate") %>'>
                                                <Calendar ID="Calendar3" Skin="Sunset" runat="server" UseColumnHeadersAsSelectors="True"
                                                    UseRowHeadersAsSelectors="True">
                                                </Calendar>
                                                <DateInput ID="DateInput3" InvalidStyleDuration="100" runat="server" LabelCssClass="radLabelCss_Sunset"
                                                    Skin="Sunset" Width="" DateFormat="MM-dd-yyyy" DisplayDateFormat="MM-dd-yyyy"
                                                    Font-Bold="true" ForeColor="Black">
                                                </DateInput>
                                                <DatePopupButton CssClass="" />
                                            </telerik:RadDatePicker>
                                            <asp:RequiredFieldValidator ID="RequiredFieldValidator2" runat="server" ControlToValidate="rdtpTripDate"
                                                SetFocusOnError="true" Display="Dynamic" ErrorMessage="Enter Trip Date"></asp:RequiredFieldValidator>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Type of Trip:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadComboBox ID="rcmbTripType" runat="server" DataSourceID="sdsTripTypes"
                                                DataTextField="TripType" DataValueField="TripTypeID" MarkFirstMatch="true" SelectedValue='<%# Eval("TripTypeID") %>'>
                                            </telerik:RadComboBox>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Trip Start:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadDatePicker ID="rdtpTripStart" runat="server" TabIndex="3" Culture="English (United States)"
                                                Skin="Sunset" DbSelectedDate='<%# Bind("TripStart") %>'>
                                                <Calendar ID="Calendar1" Skin="Sunset" runat="server" UseColumnHeadersAsSelectors="True"
                                                    UseRowHeadersAsSelectors="True">
                                                </Calendar>
                                                <DateInput ID="DateInput1" InvalidStyleDuration="100" runat="server" LabelCssClass="radLabelCss_Sunset"
                                                    Skin="Sunset" Width="" DateFormat="MM-dd-yyyy" DisplayDateFormat="MM-dd-yyyy"
                                                    Font-Bold="true" ForeColor="Black">
                                                </DateInput>
                                                <DatePopupButton CssClass="" />
                                            </telerik:RadDatePicker>
                                            <asp:RequiredFieldValidator ID="RequiredFieldValidator5" runat="server" ControlToValidate="rdtpTripStart"
                                                SetFocusOnError="true" Display="Dynamic" ErrorMessage="Enter Trip Start Date"></asp:RequiredFieldValidator>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Trip End:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadDatePicker ID="rdtpTripEnd" runat="server" TabIndex="3" Culture="English (United States)"
                                                Skin="Sunset" DbSelectedDate='<%# Bind("TripEnd") %>'>
                                                <Calendar ID="Calendar2" Skin="Sunset" runat="server" UseColumnHeadersAsSelectors="True"
                                                    UseRowHeadersAsSelectors="True">
                                                </Calendar>
                                                <DateInput ID="DateInput2" InvalidStyleDuration="100" runat="server" LabelCssClass="radLabelCss_Sunset"
                                                    Skin="Sunset" Width="" DateFormat="MM-dd-yyyy" DisplayDateFormat="MM-dd-yyyy"
                                                    Font-Bold="true" ForeColor="Black">
                                                </DateInput>
                                                <DatePopupButton CssClass="" />
                                            </telerik:RadDatePicker>
                                            <asp:RequiredFieldValidator ID="RequiredFieldValidator6" runat="server" ControlToValidate="rdtpTripEnd"
                                                SetFocusOnError="true" Display="Dynamic" ErrorMessage="Enter Trip Start Date"></asp:RequiredFieldValidator>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Start Odometer:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadNumericTextBox ID="txtStartOdometer" runat="server" Type="Number" InvalidStyleDuration="100"
                                                Text='<%# Eval("StartOdometer") %>'>
                                                <NumberFormat AllowRounding="True" KeepNotRoundedValue="False" />
                                            </telerik:RadNumericTextBox>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            End Odometer:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadNumericTextBox ID="txtEndOdometer" runat="server" Type="Number" InvalidStyleDuration="100"
                                                Text='<%# Eval("EndOdometer") %>'>
                                                <NumberFormat AllowRounding="True" KeepNotRoundedValue="False" />
                                            </telerik:RadNumericTextBox>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle" colspan="2">
                                            <asp:ImageButton ID="imgUpdate" runat="server" CommandName='<%# (Container is GridEditFormInsertItem) ? "PerformInsert" : "Update" %>'
                                                AlternateText="Update" ToolTip="Update" ImageUrl="~/App_Themes/Blue/images/savebutton.gif" />
                                            <asp:ImageButton ID="imgCancel" runat="server" CommandName="Cancel" AlternateText="Cancel"
                                                ToolTip="Cancel" ImageUrl="~/App_Themes/Blue/images/cancelbutton.gif" CausesValidation="false" />
                                        </td>
                                    </tr>
                                </table>
                            </FormTemplate>
                        </EditFormSettings>
                        <HeaderStyle HorizontalAlign="Center" />
                        <Columns>
                            <telerik:GridEditCommandColumn ButtonType="ImageButton" EditImageUrl="~/App_Themes/Blue/images/edit.gif"
                                ItemStyle-Width="5px" HeaderText="Edit">
                                <HeaderStyle Width="5px" />
                                <ItemStyle HorizontalAlign="Center" />
                            </telerik:GridEditCommandColumn>
                            <telerik:GridBoundColumn DataField="TruckNo" FilterControlAltText="Filter TruckNo column"
