                                            </telerik:RadTimePicker>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td align="left" valign="middle">
                                            Departure Time:
                                        </td>
                                        <td align="left" valign="middle">
                                            <telerik:RadTimePicker ID="rtpDepartureTime" runat="server" Skin="Sunset" DbSelectedDate='<%# Bind("DepartureTime") %>'>
